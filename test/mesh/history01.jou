#!python
cubit.cmd('reset')
cubit.cmd('#element_length = {element_length = 1.0 / 1}')
cubit.cmd('create surface rectangle width 1 zplane')
cubit.cmd('surface 1 size {element_length}')
cubit.cmd('mesh surface 1')
cubit.cmd('block 1 add surface 1')
cubit.cmd('block 1 element type quad4')
cubit.cmd('sideset 1 add curve 1')
cubit.cmd('sideset 2 add curve 2')
cubit.cmd('sideset 3 add curve 3')
cubit.cmd('sideset 4 add curve 4')
cubit.cmd('nodeset 1 add curve in sideset 1')
cubit.cmd('nodeset 2 add curve in sideset 2')
cubit.cmd('nodeset 3 add curve in sideset 3')
cubit.cmd('nodeset 4 add curve in sideset 4')
cubit.cmd('export genesis \'mesh_test_{element_length}.g\' overwrite')
